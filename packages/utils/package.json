{
  "name": "@personal-ai/utils",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "dependencies": {
    "@personal-ai/models": "workspace:*"
  },
  "devDependencies": {
    "@personal-ai/config-eslint": "workspace:*",
    "@personal-ai/config-jest": "workspace:*",
    "@personal-ai/config-typescript": "workspace:*",
    "@types/jest": "^29.5.14",
    "eslint": "^9",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "tsup": "^8.0.2",
    "typescript": "^5"
  }
} 