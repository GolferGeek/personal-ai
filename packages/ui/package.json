{
  "name": "@personal-ai/ui",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react --external @mui/material",
    "dev": "tsup src/index.tsx --format esm,cjs --dts --watch --external react --external @mui/material",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@personal-ai/models": "workspace:*"
  },
  "devDependencies": {
    "@personal-ai/config-eslint": "workspace:*",
    "@personal-ai/config-jest": "workspace:*",
    "@personal-ai/config-typescript": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.3.1",
    "tsup": "^8.0.2",
    "typescript": "^5"
  }
} 