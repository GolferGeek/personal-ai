{
  "name": "personal-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "dev:legacy": "next dev",
    "dev:server:legacy": "cd server && npm run start:dev",
    "dev:all:legacy": "concurrently \"npm run dev:legacy\" \"npm run dev:server:legacy\"",
    "build:legacy": "next build",
    "start:legacy": "next start",
    "lint:legacy": "next lint",
    "test:legacy": "jest",
    "test:watch:legacy": "jest --watch",
    "test:server:legacy": "cd server && npm test",
    "test:all:legacy": "npm test:legacy && npm run test:server:legacy"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@tanstack/react-query": "^5.72.0",
    "@types/uuid": "^10.0.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.3.1",
    "ts-node-dev": "^2.0.0",
    "turbo": "^2.0.1",
    "typescript": "^5"
  },
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
